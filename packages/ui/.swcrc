{
  "$schema": "http://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": false
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "development": true
      }
    },
    "minify": {
      "compress": true,
      "mangle": true
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false
  }
}
