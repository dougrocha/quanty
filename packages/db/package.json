    "dev": "yarn with-env prisma studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1"
  },
  "devDependencies": {
    "@quanty/tsconfig": "*",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.0.0",
    "prisma": "^4.10.1",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "zod-prisma": "^0.5.4"
  }
}
