{
  "module": {
    "type": "commonjs",
    "strict": true
  },
  "jsc": {
    "externalHelpers": false,
    "target": "es2021",
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "baseUrl": "./src",

    "keepClassNames": true,

    "minify": {
      "compress": {
        "unused": true
      },
      "mangle": true
    }
  },
  "minify": true
}
