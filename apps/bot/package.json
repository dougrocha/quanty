{
  "name": "app-bot",
  "version": "0.1.0-dev",
  "description": "A powerful discord bot built to provide features for both moderators and users",
  "main": "scr/index.ts",
  "author": "Douglas Rocha <79188161+slashtp3@users.noreply.github.com>",
  "license": "MIT",
  "repository": "https://github.com/slashtp3/Quanty.git",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "predev": "rimraf dist",
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-prisma\" \"yarn watch-dev\" \"yarn watch-tsc\"",
    "debug": "ts-node --debug -r dotenv/config src/index.ts",
    "watch-compile": "swc src -w -s -d ./dist",
    "watch-prisma": "prisma generate --watch",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "watch-tsc": "tsc -w --pretty --skipLibCheck --noEmit",
    "start": "cross-env production=true node --experimental-specifier-resolution=node ./dist/src/index.js",
    "test": "echo TESTING BOT",
    "lint": "cross-env TIMING=1 eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "mongogen": "mtgen ./src/schemas -o ./src/types/",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@quanty/framework": "*",
    "@quanty/schemas": "*",
    "@typegoose/typegoose": "^9.8.1",
    "axios": "^0.27.2",
    "better-erela.js-apple": "^1.0.2",
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "dayjs": "^1.11.1",
    "discord-html-transcripts": "~2.4.5",
    "discord.js": "^13.8.0",
    "erela.js": "^2.3.3",
    "erela.js-spotify": "^1.2.0",
    "glob": "^7.2.0",
    "mongoose": "^6.3.4",
    "nekos.life": "^3.0.0",
    "socket.io": "^4.5.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.204",
    "chokidar": "^3.5.3",
    "concurrently": "^7.2.2",
    "config": "*",
    "cross-env": "^7.0.3",
    "node-abort-controller": "^3.0.1",
    "prisma": "^3.15.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "2.0.0-0",
    "tsconfig": "*",
    "typescript": "^4.7.2"
  }
}
