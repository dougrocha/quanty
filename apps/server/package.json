{
  "name": "app-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "test:dev": "node -r @swc/register src/main.ts",
    "debug": "nest start --debug --watch",
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-prisma\" \"yarn watch-dev\" \"yarn watch-tsc\"",
    "watch-compile": "swc src -w -s -d ./dist",
    "watch-prisma": "prisma generate --watch",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "watch-tsc": "tsc -w --pretty --skipLibCheck --noEmit",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "cross-env TIMING=1 eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write src",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.14",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.6",
    "@nestjs/graphql": "^10.0.15",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.1.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/platform-socket.io": "^8.4.3",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/websockets": "^8.4.3",
    "@prisma/client": "^3.15.2",
    "@types/express-session": "^1.17.4",
    "@types/passport": "^1.0.8",
    "apollo-server-express": "^3.8.2",
    "body-parser": "^1.20.0",
    "cache-manager": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "discord-api-types": "^0.34.0",
    "express-session": "^1.17.3",
    "graphql": "^16.5.0",
    "graphql-iso-date": "^3.6.1",
    "joi": "^17.6.0",
    "mongoose": "^6.3.4",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "prisma-nestjs-graphql": "^16.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "stripe": "^9.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.3",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.204",
    "@swc/register": "^0.1.10",
    "@types/cache-manager": "^3.4.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/passport-discord": "^0.1.5",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.2.2",
    "config": "*",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.16",
    "prisma": "^3.15.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.7.0",
    "tsconfig": "*",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "/^src/(.*)$/": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
