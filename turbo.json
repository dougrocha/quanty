{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "build": {
      "outputs": ["dist/**", ".next/**", "lib/**"],
      "cache": true
    },
    "app-bot#build": {
      "cache": true
    },
    "app-bot#start": {
      "dependsOn": ["app-bot#build"]
    },
    "app-backend#build": {
      "outputs": ["dist/**"],
      "cache": true
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
